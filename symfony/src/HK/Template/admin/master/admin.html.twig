<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8"/>
		<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport"/>
		<title>
			{% block page_title %}
				{{'lbl.admin-title'|trans}}
			{% endblock %}
		</title>
		<meta
		content="HK" name="description"/>

		<!-- Favicon -->
		<link href="cms-favicon.ico" rel="shortcut icon"/>
		<link
		href="cms-favicon.ico" rel="icon" type="image/x-icon"/>

		<!-- Toggles CSS -->
		<link href="/assets/plugins/jquery-toggles/css/toggles.css" rel="stylesheet" type="text/css"/>
		<link
		href="/assets/plugins/jquery-toggles/css/themes/toggles-light.css" rel="stylesheet" type="text/css"/>

		<!-- Toastr CSS -->
		<link
		href="/assets/plugins/jquery-toast-plugin/dist/jquery.toast.min.css" rel="stylesheet" type="text/css"/>
		<!-- Custom CSS -->
		<link href="/assets/admin/css/style.css" rel="stylesheet" type="text/css"/>

		<link href="/assets/plugins/bootstrap-datepicker-1.9.0/css/bootstrap-datepicker.min.css" rel="stylesheet" media="screen">

		<link href="/assets/admin/css/theme-custom.css" rel="stylesheet" type="text/css"/>

		<link href="/assets/admin/css/common.css" rel="stylesheet" type="text/css"/> {% block css %}{% endblock %}
		</head>
		<body>
			{# <div class="preloader-it" style="backgroud: none">
				<div class="loader-pendulums"></div>
			</div> #}
			<div class="modal fade" id="rb-modal-page" tabindex="-1" role="dialog" aria-labelledby="rb-modal-page-tile" rb-data-modal-text-ok="{{'lbl.modal.ok'|trans}}" rb-data-modal-text-confirm="{{'lbl.modal.confirm'|trans}}" rb-data-modal-title-confirm="{{'lbl.modal.header-confirm'|trans}}" rb-data-modal-title-ok="{{'lbl.modal.header-ok'|trans}}">
				<div class="modal-dialog" role="document">
					<div class="modal-content">
						<div class="modal-header">
							<h5 class="modal-title" id="rb-modal-page-title"></h5>
							<button type="button" class="close" data-dismiss="modal" aria-label="Close">
								<span aria-hidden="true">
									×
								</span>
							</button>
						</div>
						<div class="modal-body">
							<p id="rb-modal-page-content"></p>
						</div>
						<div class="modal-footer">
							<button type="button" class="btn btn-secondary" id="rb-modal-page-cancel" data-dismiss="modal">
								{{'lbl.modal.cancel'|trans}}
							</button>
							<button type="button" class="btn btn-primary" id="rb-modal-page-ok"></button>
						</div>
					</div>
				</div>
			</div>
			{% if data.has_photo_modal is defined and data.has_photo_modal != '0' %}
				<div class="modal fade" id="rb-modal-media" tabindex="-1" role="dialog">
					<div class="modal-dialog" role="document" style="max-width: 80%; min-width: 1050px">
						<div class="modal-content border-0">
							<div class="modal-header border-0">
								<button type="button" class="close" data-dismiss="modal" aria-label="Close">
									<span aria-hidden="true">
										×
									</span>
								</button>
							</div>
							<div class="modal-body">
								<iframe src="{{path('hk_admin.mediafile.subin')}}" style="width: 100%; height: 100%; min-height: 600px; border: none"></iframe>
							</div>

						</div>
					</div>
				</div>
			{% endif %}
			<!-- HK Wrapper -->
			<div class="hk-wrapper hk-vertical-nav">
				{{ render(controller('HK\\AdminBundle\\Controller\\CommonController::navTop', {})) }}
				{{ render(controller('HK\\AdminBundle\\Controller\\CommonController::navLeft', {})) }}
				<!-- Main Content -->
				<div
					class="hk-pg-wrapper">

					<!-- Container -->
					<div
						class="container mt-10">
						<!-- Title -->
						<div class="hk-pg-header">
							<h4 class="hk-pg-title">
								<span class="pg-title-icon">
									<span class="feather-icon">
										<i data-feather="{{data.controller_icon}}"></i>
									</span>
								</span>
								{{data.controller_text|trans}}
								{% if data.page_header is defined and data.page_header is not empty %}
									<i class="fas fa-angle-right ml-20 mr-20"></i>
									{{data.page_header}}
								{% endif %}
							</h4>
							{% block titleInfo %}{% endblock %}
						</div>
						<!-- /Title -->
						{% block body_content %}{% endblock %}
						{{ include('admin/common/footer.html.twig') }}
					</div>
					<!-- /Container -->
				</div>
				<!-- /Main Content -->
			</div>
			<!-- jQuery -->
			<script src="/assets/plugins/jquery-3.4.0/dist/jquery.min.js"></script>

			<!-- Bootstrap Core JavaScript -->
			<script src="/assets/plugins/popper.js/dist/umd/popper.min.js"></script>
			<script src="/assets/plugins/bootstrap-4.3.1/dist/js/bootstrap.min.js"></script>

			<!-- Slimscroll JavaScript -->
			<script src="/assets/admin/js/jquery.slimscroll.js"></script>

			<!-- Fancy Dropdown JS -->
			<script src="/assets/admin/js/dropdown-bootstrap-extended.js"></script>

			<!-- FeatherIcons JavaScript -->
			<script src="/assets/admin/js/feather.min.js"></script>

			<!-- Toggles JavaScript -->
			<script src="/assets/plugins/jquery-toggles/toggles.min.js"></script>
			<!-- Toastr JS -->
			<script src="/assets/plugins/jquery-toast-plugin/dist/jquery.toast.min.js"></script>


			<script type="text/javascript" src="/assets/plugins/bootstrap-datepicker-1.9.0/js/bootstrap-datepicker.js" charset="UTF-8"></script>

			<!-- CKEditor -->
			<script src="/assets/plugins/ckeditor4/ckeditor.js"></script>

			<script src="/assets/admin/js/hk-init-min.js"></script>
			<script src="/assets/admin/js/hk-data-min.js"></script>
			<script src="/assets/admin/js/hk-common-min.js"></script>
			<script src="/assets/admin/js/hk-jgrid-min.js"></script>
			<script src="/assets/admin/js/hk-admin-min.js"></script>
			<script type="text/javascript">
				$(function () {
$('.navbar-nav-wrap ul li.nav-item > a').each(function () {
var $ob = $(this);
if ((window.location.pathname + '/').indexOf($ob.attr('href') + '/') >= 0) {
$ob.addClass('-active');
$ob.parent().parent().parent().parent().parent().find('>a').trigger('click');
};
});
});
			</script>

			{% block script %}{% endblock %}
		</body>
	</html>
